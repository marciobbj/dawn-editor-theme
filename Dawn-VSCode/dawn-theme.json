  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#bebebe",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#d25b00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#fee872"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#ffa500"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "support.class",
        "meta.class"
      ],
      "settings": {
        "foreground": "#ffa500"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.language"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#d25b00"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#d25b00"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#fee872"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "meta.annotation"
      ],
      "settings": {
        "foreground": "#fee872"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#ffa500"
      }
    },
    {
      "name": "Built-in Functions",
      "scope": [
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Errors",
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#e06c75",
        "background": "#51202a"
      }
    }
  ]
}
